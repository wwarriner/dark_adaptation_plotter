{
  "arrow_head_size_pt": {
    "object": "uispinner",
    "config": "plot.arrow.head_size_pt",
    "label": { "Text": "Arrow head size (pt)" },
    "ui": {
      "Value": { "config": "plot.arrow.head_size_pt.value" },
      "Limits": [1, 72],
      "Step": 1,
      "RoundFractionalValues": true,
      "ValueDisplayFormat": "%d",
      "LowerLimitInclusive": true,
      "UpperLimitInclusive": true
    }
  },
  "arrow_line_width_pt": {
    "object": "uispinner",
    "config": "plot.arrow.line_width_pt",
    "label": { "Text": "Arrow line width (pt)" },
    "ui": {
      "Value": { "config": "plot.arrow.line_width_pt.value" },
      "Limits": [0.5, 6],
      "Step": 0.5,
      "RoundFractionalValues": false,
      "ValueDisplayFormat": "%.1f",
      "LowerLimitInclusive": true,
      "UpperLimitInclusive": true
    }
  },
  "legend_location": {
    "object": "uidropdown",
    "config": "axes.legend.location",
    "label": { "Text": "Legend location" },
    "ui": {
      "Items": [
        "North",
        "South",
        "East",
        "NorthEast",
        "SouthEast",
        "NorthOutside",
        "SouthOutside",
        "EastOutside",
        "Best",
        "BestOutside"
      ],
      "Value": { "config": "axes.legend.location.value" }
    }
  },
  "x_axis_title": {
    "object": "uieditfield",
    "config": "axes.x.label",
    "label": { "Text": "X axis title" },
    "ui": {
      "Value": { "config": "axes.x.label.value" }
    }
  },
  "x_max": {
    "object": "uispinner",
    "config": "axes.x.max",
    "label": { "Text": "X axis maximum" },
    "ui": {
      "Value": { "config": "axes.x.max.value" },
      "Limits": [{ "config": "axes.x.min.value" }, { "float": "inf" }],
      "Step": { "config": "axes.x.step.value" },
      "RoundFractionalValues": true,
      "ValueDisplayFormat": "%.2f",
      "LowerLimitInclusive": false,
      "UpperLimitInclusive": false
    }
  },
  "y_axis_title": {
    "object": "uieditfield",
    "config": "axes.y.label",
    "label": { "Text": "Y axis title" },
    "ui": {
      "Value": { "config": "axes.y.label.value" }
    }
  },
  "y_min": {
    "object": "uispinner",
    "config": "axes.y.min",
    "label": { "Text": "Y axis minimum" },
    "ui": {
      "Value": { "config": "axes.y.min.value" },
      "Limits": [{ "float": "-inf" }, { "config": "axes.y.max.value" }],
      "Step": { "config": "axes.y.step.value" },
      "RoundFractionalValues": false,
      "ValueDisplayFormat": "%.2f",
      "LowerLimitInclusive": false,
      "UpperLimitInclusive": false
    }
  },
  "y_max": {
    "object": "uispinner",
    "config": "axes.y.max",
    "label": { "Text": "Y axis maximum" },
    "ui": {
      "Value": { "config": "axes.y.max.value" },
      "Limits": [{ "config": "axes.y.min.value" }, { "float": "inf" }],
      "Step": { "config": "axes.y.step.value" },
      "RoundFractionalValues": false,
      "ValueDisplayFormat": "%.2f",
      "LowerLimitInclusive": false,
      "UpperLimitInclusive": false
    }
  }
}
